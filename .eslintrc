{
  "extends": "react-app",
  "rules": {
    "comma-dangle": [1, "always-multiline"],
    "eol-last": 1,
    "jsx-quotes": 1,
    "key-spacing": 1,
    "keyword-spacing": 1,
    "no-multi-spaces": 1,
    "no-trailing-spaces": 1,
    "no-unexpected-multiline": 0,
    "no-unused-vars": 1,
    "object-curly-spacing": [1, "always"],
    "quotes": ["warn", "single", { "avoidEscape": true }],
    "semi": 1,
    "space-before-function-paren": 0
  }
}
